{
  "version": "1.0",
  "description": "Configuracion Multi-Proveedor para Robot Framework v1.2+",
  "last_updated": "2025-07-09",
  "providers": {
    "gemini": {
      "name": "Google Gemini",
      "description": "Google Generative AI - Gemini Models",
      "api_key_var": "GEMINI_API_KEY",
      "base_url": "https://generativelanguage.googleapis.com/v1beta/models",
      "models": [
        "gemini-1.5-flash",
        "gemini-1.5-pro",
        "gemini-pro"
      ],
      "default_model": "gemini-1.5-flash",
      "enabled": true,
      "priority": 1,
      "features": {
        "text_generation": true,
        "code_generation": true,
        "analysis": true,
        "chat": true
      }
    },
    "claude": {
      "name": "Anthropic Claude",
      "description": "Anthropic Claude AI Assistant",
      "api_key_var": "CLAUDE_API_KEY",
      "base_url": "https://api.anthropic.com/v1/messages",
      "models": [
        "claude-3-5-sonnet-20241022",
        "claude-3-haiku-20240307"
      ],
      "default_model": "claude-3-5-sonnet-20241022",
      "enabled": true,
      "priority": 2,
      "features": {
        "text_generation": true,
        "code_generation": true,
        "analysis": true,
        "chat": true,
        "large_context": true
      }
    },
    "aws": {
      "name": "AWS Bedrock",
      "description": "Amazon Web Services Bedrock AI",
      "api_key_var": "AWS_ACCESS_KEY_ID",
      "secret_key_var": "AWS_SECRET_ACCESS_KEY",
      "region_var": "AWS_DEFAULT_REGION",
      "base_url": "https://bedrock-runtime.us-east-1.amazonaws.com",
      "models": [
        "anthropic.claude-3-sonnet-20240229-v1:0",
        "amazon.titan-text-express-v1",
        "meta.llama2-70b-chat-v1"
      ],
      "default_model": "anthropic.claude-3-sonnet-20240229-v1:0",
      "enabled": false,
      "priority": 3,
      "features": {
        "text_generation": true,
        "code_generation": true,
        "analysis": true,
        "enterprise": true
      }
    }
  },
  "settings": {
    "active_provider": "claude",
    "fallback_enabled": true,
    "fallback_order": [
      "gemini",
      "claude",
      "aws"
    ],
    "timeout_seconds": 30,
    "max_retries": 3,
    "debug_mode": false
  },
  "compatibility": {
    "robot_framework_version": ">=6.0",
    "python_version": ">=3.8",
    "maintains_v12_compatibility": true,
    "gemini_library_compatible": true
  }
}